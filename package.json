{
  "name": "@thekarel/spiral",
  "description": "A tiny helper for Linear git workflow",
  "version": "0.0.3",
  "author": "Charles Szilagyi",
  "bin": {
    "spiral": "./bin/run.js"
  },
  "private": false,
  "bugs": "https://github.com/thekarel/spiral/issues",
  "dependencies": {
    "@linear/sdk": "^51.1.0",
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "open-editor": "^5.1.0",
    "shelljs": "^0.10.0",
    "yoctocolors": "^2.1.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/node": "^18",
    "@types/shelljs": "^0.8.17",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/thekarel/spiral",
  "keywords": [
    "oclif"
  ],
  "license": "ISC",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "spiral",
    "dirname": "spiral",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "repository": "thekarel/spiral",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
